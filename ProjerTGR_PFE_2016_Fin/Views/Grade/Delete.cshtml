@model ProjerTGR_PFE_2016_Fin.Models.Grade

@{
    ViewBag.Title = "Delete";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title"> <i class="fa fa-trash-o" aria-hidden="true"></i> Delete </h4>
</div>
<div class="modal-body">

@using (Html.BeginForm("Delete", "Grade", FormMethod.Post))
        {
    @Html.AntiForgeryToken()
    <fieldset>
        <legend>Voulez vous vraiment supprimer ce Grade !!!</legend>


        <table>
            <tr>
                <td class="display-label" id="td_label">@Html.DisplayNameFor(model => model.Num_gard) :</td>
                <td class="display-field" id="td_out">@Html.DisplayFor(model => model.Num_gard)</td>
            </tr>
            <tr>
                <td class="display-label" id="td_label">@Html.DisplayNameFor(model => model.Nom_grad) :</td>
                <td class="display-field" id="td_out">@Html.DisplayFor(model => model.Nom_grad)</td>
            </tr>
            <tr>
                <td class="display-label" id="td_label">@Html.DisplayNameFor(model => model.Corps.Nom_corp):</td>
                <td class="display-field" id="td_out">@Html.DisplayFor(model => model.Corps.Nom_corp)</td>
            </tr>
        </table>


       
    </fieldset>
    <div class="modal-footer">
        
            <p>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                <input type="submit" class="btn btn-primary" value="Delete" /> 
            </p>
      


    </div>
    
}
</div>
